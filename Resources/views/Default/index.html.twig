    <div class="section section-dark">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="calltoaction-wrapper">


                        <span>
                            <h3 {{ createphp_attributes( rdf.subtitle ) }}>{{ createphp_content( rdf.subtitle ) }}</h3>
                        </span>

                        <a href="https://github.com/7rin0/SevenManagerBundle"
