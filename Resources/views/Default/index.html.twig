                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Call to Action Bar -->
    <div class="section section-white">
        <div class="container" {{ createphp_attributes( rdf.body ) }}>
            {{ createphp_content( rdf.body ) }}
        </div>
    </div>

